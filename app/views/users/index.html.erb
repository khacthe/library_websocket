<% provide(:title, t("users.list_users")) %>
<div class="list_users main">
  <div class="container">
    <div class="row">
     <%= form_tag users_path, method: :get, id: "book_search" do %>
        <div class="form_search" >
          <%= text_field_tag :search, params[:search] %>
          <%= submit_tag t "users.search", name: nil %>
        </div>
      <% end %>
      <% if @users.present? %>
        <ul>
          <%= render @users %>
        </ul>
      <% else %>
        <h5 class="not_avail"><%= t "home.resuft_not_avail" %></h5>
      <% end %>
      <%= will_paginate @users %>
    </div>
  </div>
</div>
